{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:report": "npx eslint . --ext .js,.jsx,.ts,.tsx -f node_modules/eslint-html-reporter/reporter.js -o report.html",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --ignore-path .gitignore --check \"**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:write": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,css,scss}\"",
    "prepare": "cd .. && husky install client/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "axios": "^1.2.1",
    "facepaint": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/facepaint": "^1.2.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.1",
    "prettier": "2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-require": "^1.1.10",
    "vite-plugin-svgr": "^2.4.0"
  }
}
